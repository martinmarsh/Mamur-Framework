<?xml version="1.0" standalone="yes"?>
<page>

<!--     PAGE
 This page uses sygenius page server DOM
 This XML file defines how the page should appear.
 ALL xml elements between <page> and </page> are optional.
 If omitted either defaults or values defined in the template will be used.
 A title element should be defined because each page should have a unique pages title.


-->
    <template file="main_template" />

    <!-- title should be in special title tag to allow future editing -->
    <title>Home Page</title>

    <!-- meta should should be in special meta to allow future editing
         any number allowed
    -->
    <meta name="description" content="Home page description" />
    <meta name="keywords"   content="Home, page, keywords" />
    <meta http-equiv="expires" content="-1" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Pragma" content="no-cache" />


</page>